 public class Main {
    public static void main(String[] args){
Animal animalObj =new Animal();
animalObj.setName("tiger");
animalObj.setAge(5);
System.out.println("name of the animaL IS:"+animalObj.getName());
System.out.println("age of the animal is:"+animalObj.getAge());
    }
}
class Animal{
   private String name;
   private int age;
    public String getName(){
        return name;
    }
    public void setName(String name){
        this.name =name;
    }
    public int getAge(){
        return age;
    }
    public void setAge(int age){
        this.age=age;
    }
}
